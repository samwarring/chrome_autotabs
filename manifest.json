{
    "manifest_version": 3,
    "name": "Tab Harmony",
    "version": "0.1.1",
    "description": "A place for every tab, and every tab in its place.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background2.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabGroups",
        "storage"
    ]
}