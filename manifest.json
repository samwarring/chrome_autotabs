{
    "manifest_version": 3,
    "name": "Tab Organizer",
    "version": "1.0",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabGroups"
    ]
}